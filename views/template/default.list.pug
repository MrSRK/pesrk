.mt-4(data-ng-init="prepear('wlist','"+m+"')")
    .row.justify-content-start(data-ng-init="handlers['"+m+"'].get()")
        .col-6.col-md-4.col-lg-2.mb-4(data-ng-repeat="row in handlers['"+m+"'].data")
            .card
                img.card-img-top(data-ng-src='/static/{{row.images[0].location}}/thumbnail.webp' alt='{{row.name}}')
                .card-body
                    h5.mb-0.card-title.small(data-ng-bind='row.title')
                    h6.mb-3.small 
                        small.text-muted(data-ng-bind="row.user.name")
                    .text-right
                        a.btn.btn-primary(data-ng-href=m+'/{{row._id}}') Περισσότερα